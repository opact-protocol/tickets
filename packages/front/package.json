{
  "name": "@near/front",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "add_zk_assets": "yarn add_verifier_wasm && yarn add_circuit",
    "add_verifier_wasm": "cp ../circuits/out/withdraw_js/withdraw.wasm ./public/verifier.wasm",
    "add_circuit": "cp ../circuits/out/withdraw_0000.zkey ./public/circuit.zkey",
    "check_is_circuit_compiled": "test -f ../circuits/out/withdraw_js/withdraw.wasm || 'Failure: need to compile circuit first. Go to packages/circuits and run yarn circuit:setup:plonk'",
    "vercel-dev-helper": "vite --port $PORT",
    "vdev": "vercel dev --local-config ./vercel-dev.json",
    "dev": "(cd ../sdk && yarn build) && yarn check_is_circuit_compiled && yarn add_zk_assets && vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "engines": {
    "yarn": ">=1.22.0 <2.0.0",
    "node": ">=17.5.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^2.0.4",
    "@koale/useworker": "^4.0.2",
    "@metamask/eth-sig-util": "^7.0.1",
    "@near-wallet-selector/core": "^7.0.3",
    "@near-wallet-selector/here-wallet": "^7.8.2",
    "@near-wallet-selector/meteor-wallet": "^7.0.3",
    "@near-wallet-selector/my-near-wallet": "^7.8.0",
    "@near-wallet-selector/near-wallet": "^7.0.3",
    "@near-wallet-selector/nightly": "^7.8.2",
    "@near-wallet-selector/sender": "^7.8.2",
    "@near-wallet-selector/welldone-wallet": "^7.8.2",
    "@near-wallet-selector/xdefi": "^7.8.2",
    "@rollup/plugin-inject": "^4.0.4",
    "@sentry/react": "^7.38.0",
    "@sentry/tracing": "^7.38.0",
    "@vercel/node": "^2.9.12",
    "autoprefixer": "^10.4.7",
    "big.js": "^6.2.1",
    "buffer": "^6.0.3",
    "clsx": "^1.2.1",
    "file-saver": "^2.0.5",
    "graphql": "^16.8.0",
    "hideyourcash-sdk": "*",
    "js-base64": "^3.7.5",
    "localforage": "^1.10.0",
    "node-stdlib-browser": "^1.2.0",
    "opact-sdk": "^1.0.96",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.0.0",
    "react-download-link": "^2.3.0",
    "react-gtm-module": "^2.0.11",
    "react-reveal": "^1.2.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.1.1",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.0.24",
    "vite-plugin-pages": "^0.25.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/react": "^18.0.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-gtm-module": "^2.0.1",
    "@vitejs/plugin-react": "^1.3.0",
    "fs": "^0.0.1-security",
    "graphql-request": "5.2.0",
    "rollup": "^2.79.0",
    "rollup-plugin-node-builtins": "^2.1.2",
